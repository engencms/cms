{
    "name": "engencms/engen",
    "description": "A static site generator and CMS",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Magnus Eriksson",
            "email": "mange@reloop.se"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "enstart/core": "dev-develop",
        "symfony/console": "^3.3",
        "maer/file-db": "^0.3.1",
        "erusev/parsedown": "^1.6",
        "wapmorgan/file-type-detector": "^1.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.8"
    },
    "scripts": {
        "cs": "./vendor/bin/phpcs --standard=PSR2 --extensions=php --ignore=vendor/,tests/,public/,data/,build/ -n -s ./",
        "test": [
            "@cs"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Engen\\": "dev/core/src"
        }
    }
}
